# Kickstart file automatically generated by anaconda.

install
key a2139dc7a29fd59d
url --url http://@@KSIPADDR@@/installs/cent/5Server/en/os/i386/iso
lang en_US.UTF-8
keyboard us
skipx
network --bootproto dhcp
rootpw --iscrypted $1$AFNYZP36$9iMbuZ7RvhkTtYgpswPkc.
firewall --disabled
selinux --disabled
authconfig --enableshadow --enablemd5
timezone --utc America/Chicago
bootloader --location=mbr

%include /tmp/part-include


# Anaconda no longer allows comments being placed on the same line as a package.  Please
# refrain from adding comments next to a package name
%packages
@ admin-tools
@ development-libs
@ development-tools
@ dns-server
@ editors
@ ftp-server
@ legacy-network-server
@ legacy-software-development                                 
@ legacy-software-support
@ mail-server
@ mysql
@ server-cfg
@ smb-server
@ sql-server
@ system-tools
@ text-internet
@ web-server

kernel
# MySQL optional pacakges
php-mysql
mod_auth_mysql
# web-server optional pacakges
libdbi-dbd-pgsql
perl-Crypt-SSLeay
perl-libwww-perl
